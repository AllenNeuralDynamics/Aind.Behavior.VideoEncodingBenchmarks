                        </Expression>
                        <Expression xsi:type="MemberSelector">
                          <Selector>TriggeredCameraController1</Selector>
                        </Expression>
                        <Expression xsi:type="IncludeWorkflow" Path="Extensions\InstantitateTriggeredCameras.bonsai">
                          <Camera>CameraOutput1</Camera>
                        </Expression>
                        <Expression xsi:type="rx:GroupBy">
                          <rx:KeySelector>Item2</rx:KeySelector>
                          <rx:ElementSelector>Item1</rx:ElementSelector>
                        </Expression>
                        <Expression xsi:type="rx:ReplaySubject">
                          <Name>TriggeredCamerasStream1</Name>
                          <rx:BufferSize xsi:nil="true" />
